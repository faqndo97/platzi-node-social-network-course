{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@now/node"
    },
    {
      "src": "posts/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/users(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/posts(.*)",
      "dest": "/api/index-posts.js"
    }
  ]
}